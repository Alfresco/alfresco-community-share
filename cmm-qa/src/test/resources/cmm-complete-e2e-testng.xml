<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="CMMRegressionTestSuite" verbose="6" preserve-order="true">
<tests>
	<test name="regression-tests-cmis">
		<classes>
			<class name="org.alfresco.test.cmm.regression.DynamicModelCmisTest"/>
			<class name="org.alfresco.test.cmm.regression.PropertyWithConstraintCMISTest"/>			
		</classes>
	</test> 
	<test name="regression-tests-models">
		<classes>
			<class name="org.alfresco.test.cmm.regression.CMAdminGroupTest"/>
			<class name="org.alfresco.test.cmm.regression.ModelConstraintsTest"/>			
			<class name="org.alfresco.test.cmm.regression.ModelLifeCycleTest"/>
			<class name="org.alfresco.test.cmm.regression.ModelLifeCycleEditModelTest"/>			
		</classes>
	</test>
	<test name="regression-tests-types-aspects">
		<classes>
			<class name="org.alfresco.test.cmm.regression.CustomTypeLifeCycleTest"/>	
			<class name="org.alfresco.test.cmm.regression.AspectLifeCycleTest"/>				
		</classes>
	</test>    
	<test name="regression-tests-properties">
		<classes>
			<class name="org.alfresco.test.cmm.regression.PropertyVariantsTest"/>	
			<class name="org.alfresco.test.cmm.regression.PropertyUsageTest"/>
			<class name="org.alfresco.test.cmm.regression.PropertyLifeCycleTest"/>				
		</classes>
	</test>
	<test name="regression-tests-Edit-types-aspects">
		<classes>
			<class name="org.alfresco.test.cmm.regression.EditCustomTypeTest"/>	
			<class name="org.alfresco.test.cmm.regression.EditAspectTest"/>							
		</classes>
	</test>
	<test name="regression-tests-Edit-properties">
		<classes>
			<class name="org.alfresco.test.cmm.regression.EditPropertyWithoutConstraintsTest"/>	
			<class name="org.alfresco.test.cmm.regression.EditAspectPropertyWithConstraintsTest"/>	
			<class name="org.alfresco.test.cmm.regression.EditTypePropertyWithConstraintsTest"/>						
		</classes>
	</test>
	<test name="regression-tests-use-model-on-share">
		<classes>
			<class name="org.alfresco.test.cmm.regression.DynamicModelShareTest"/>				
		</classes>
	</test>   
	<test name="regression-tests-find-where-used">
		<classes>
			<class name="org.alfresco.test.cmm.regression.FindInstancesTest"/>				
		</classes>
	</test>
	<test name="regression-tests-property-constraint-test">
		<classes>
			<class name="org.alfresco.test.cmm.regression.PropertyWithConstraintShareTest"/>				
		</classes>
	</test>	
	<test name="regression-tests-property-indexing-test">
		<classes>
			<class name="org.alfresco.test.cmm.regression.PropertyWithIndexingOptionsTest"/>				
		</classes>
    </test> 
</tests>  
</suite>