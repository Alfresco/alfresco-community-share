<?xml version="1.0" encoding="UTF-8"?>
<page> 
   <id>sectionpage3</id>
   <description>section landing page with single column article list showing thumbnail images</description>      
   <template-instance>two-block</template-instance>
   <authentication>none</authentication>
   <components>
      <component>
         <region-id>left1</region-id>
         <url>/list/wide2</url>
	     <properties>
	         <collection>section.articles</collection>
	     </properties>              
      </component>    
      <component>
         <region-id>right1</region-id>
         <url>/list/links</url>
          <properties>
             <collection>related.articles</collection>
          </properties>              
      </component>             
      <component>
         <region-id>right2</region-id>
         <url>/tags/list</url>
      </component>             
   </components>
</page>
