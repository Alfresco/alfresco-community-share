<?xml version='1.0' encoding='UTF-8'?>
<ehcache>
    
    <!-- defaults -->
    <diskStore 
        path="java.io.tmpdir"
    />

    <defaultCache 
        maxElementsInMemory="5000" 
        eternal="true" 
        timeToIdleSeconds="0" 
        timeToLiveSeconds="0" 
        overflowToDisk="false" 
        statistics="false"
        >
    </defaultCache>

    <cache 
        name="assetCache" 
        maxElementsInMemory="1000" 
        eternal="true"
        overflowToDisk="false"
        statistics="false"
        />

    <cache 
        name="assetCollectionCache" 
        maxElementsInMemory="500" 
        eternal="true"
        overflowToDisk="false"
        statistics="false"
        />

    <cache 
        name="formIdCache" 
        maxElementsInMemory="1000" 
        eternal="true"
        overflowToDisk="false"
        statistics="false"
        />

</ehcache>