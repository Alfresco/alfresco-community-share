<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>

   <!-- Spring Surf -->
   <rule>
      <from>/proxy(.*)</from>
      <to>/page/proxy/$1</to>
   </rule>
   <rule>
      <from>/res/(.*)</from>
      <to>/page/resource/$1</to>
   </rule>
   <rule>
      <from>/service/(.*)</from>
      <to>/page/$1</to>
   </rule>
   <rule> 
      <from>/components/(.*)</from> 
      <to>/page/resource/components/$1</to> 
   </rule>
</urlrewrite>