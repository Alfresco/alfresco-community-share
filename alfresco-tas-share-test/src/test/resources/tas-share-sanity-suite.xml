<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ShareSmokeSuite-Docker" preserve-order="true"
       parallel="false" thread-count="1">
   <listeners>
      <listener
         class-name="org.alfresco.utility.report.HtmlReportListener" />
      <listener
         class-name="org.alfresco.utility.testng.OSTestMethodSelector" />
      <listener class-name="org.alfresco.utility.testrail.TestRailExecutorListener" />
   </listeners>
   <test name="Search">
      <classes>
         <class name="org.alfresco.share.userDashboard.WelcomePanelTests">
            <methods>
               <include name="verifyWelcomePanel" />
            </methods>
         </class>
         <class name="org.alfresco.share.LoginTests">
            <methods>
               <include name="loginValidCredentials" />
               <include name="loginInvalidCredentials" />
               <include name="loginInvalidPassword" />
            </methods>
         </class>
         <class name="org.alfresco.share.alfrescoContent.viewingContent.ViewingAFileInBrowserTests">
            <methods>
               <include name="viewingAFileInBrowser" />
            </methods>
         </class>
         <class name="org.alfresco.share.site.accessingExistingSites.EnteringSiteTests">
            <methods>
               <include name="accessSiteUsingMySitesDashlet" />
               <include name="accessSiteUsingSiteFinderPage" />
            </methods>
         </class>
         <class name="org.alfresco.share.tasksAndWorkflows.CancellingAWorkflow.CancelWorkflowsTests">
            <methods>
               <include name="cancelWorkflow" />
            </methods>
         </class>
         <class name="org.alfresco.share.searching.SearchHighlightTests">
            <methods>
               <include name="highlightSearchByName" />
               <include name="highlightSearchByTitle" />
            </methods>
         </class>
         <class name="org.alfresco.share.userProfile.UserProfileContentTests">
         </class>
      </classes>
   </test>
</suite>