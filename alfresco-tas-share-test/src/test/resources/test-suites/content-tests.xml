<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Content Tests" preserve-order="true">
   <listeners>
      <listener class-name="org.alfresco.utility.report.HtmlReportListener"></listener>
   </listeners>

   <test name="Building content">
      <groups>
         <run>
           <include name="content"></include>
         </run>
      </groups>
      <packages>
         <package name="org.alfresco.share.alfrescoContent.buildingContent.*"></package>
      </packages>
   </test>

</suite>