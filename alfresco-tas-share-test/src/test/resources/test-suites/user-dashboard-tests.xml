<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="User Dashboard Tests" preserve-order="true">
   <listeners>
      <listener class-name="org.alfresco.utility.report.HtmlReportListener"></listener>
   </listeners>

   <test name="User Dashboard Tests">
      <groups>
         <run>
           <include name="user-dashboard"></include>
         </run>
      </groups>
      <packages>
         <package name="org.alfresco.share.userDashboard.*"></package>
      </packages>
   </test>

</suite>