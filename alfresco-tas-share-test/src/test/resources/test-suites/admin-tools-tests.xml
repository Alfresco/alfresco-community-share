<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Admin Tools Test Suite" parallel="methods" thread-count="6">
   <listeners>
      <listener class-name="org.alfresco.utility.report.HtmlReportListener"/>
   </listeners>

   <test name="Admin Tools Tests">
      <groups>
         <run>
            <include name="admin-tools"/>
         </run>
      </groups>
      <packages>
         <package name="org.alfresco.share.adminTools.*"/>
      </packages>
   </test>

</suite>