<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ShareSmokeSuite-Docker" preserve-order="true"
       parallel="false" thread-count="2">
   <listeners>
      <listener class-name="org.alfresco.utility.report.HtmlReportListener" />
      <listener class-name="org.alfresco.utility.testrail.TestRailExecutorListener"></listener>
      <listener class-name="org.alfresco.utility.testng.OSTestMethodSelector"></listener>
   </listeners>
   <!-- in order for this to work you need to pass -Denvironment=docker to
      use the docker.properties -->
   <test name="Share">
      <classes>
         <class name="org.alfresco.share.alfrescoContent.applyingRulesToFolders.TransformationServerTest"></class>
      </classes>
   </test>
</suite>