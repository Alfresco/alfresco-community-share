<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
   <template-type>org/alfresco/wiki-page</template-type>
   <properties>
      <pageFamily>wiki</pageFamily>
      <container>wiki</container>
   </properties>
   <components>

      <!-- Title -->
      <component>
         <region-id>title</region-id>
         <url>/components/title/collaboration-title</url>
      </component>

      <!-- Navigation -->
      <component>
         <region-id>navigation</region-id>
         <url>/components/navigation/collaboration-navigation</url>
      </component>

      <!-- Toolbar -->
      <component>
         <region-id>toolbar</region-id>
         <url>/components/wiki/toolbar</url>
         <properties>
            <showBackLink>true</showBackLink>
         </properties>
      </component>

      <!-- Wiki Pages -->
      <component>
         <region-id>wikipage</region-id>
         <url>/components/wiki/page</url>
      </component>

   </components>
</template-instance>
